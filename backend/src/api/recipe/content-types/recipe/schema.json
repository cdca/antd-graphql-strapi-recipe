{
  "kind": "collectionType",
  "collectionName": "recipes",
  "info": {
    "singularName": "recipe",
    "pluralName": "recipes",
    "displayName": "Recipe",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "rating": {
      "type": "integer",
      "min": 1,
      "max": 10
    },
    "body": {
      "type": "richtext"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "recipes"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "uuid": {
      "type": "uid",
      "targetField": "title"
    },
    "time": {
      "type": "integer"
    },
    "ingredients": {
      "type": "richtext"
    }
  }
}
